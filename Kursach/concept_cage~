sc_node_not_relation->concept_cage;;

concept_cage
=> nrel_main_idtf:
	[ клетка ] (* <-lang_ru;; *);
	[ cage ] (* <-lang_en;; *);


<-rrel_key_sc_element:
	...
	(*
	<-sc_definition;;
	=> nrel_main_idtf: [Опр.(клетка)] (* <- lang_ru;; *);;
	<=nrel_sc_text_translation:
		...
		(*
		-> rrel_example:
		[Клетка — регулярный граф наименьшего обхвата для заданной степени вершин.] (*<-lang_ru;;*);;	
		<= nrel_using_constants:
		{
			concept_graph;
			concept_arg;
			concept_node
		};;		
		*);;
	*);

<-rrel_key_sc_element:
	... 
	(*
	=>nrel_main_idtf:[Утв.(клетка) ](*<-lang_ru;;*);;
	<- sc_statement;;
